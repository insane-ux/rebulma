{
  "name": "rebulma",
  "version": "0.1.0",
  "description": "React components starter kit, use Storybook",
  "scripts": {
    "build": "NODE_ENV=production babel ./components --out-dir ./lib --ignore *.test.js,stories.js",
    "flow": "npm run lint && flow",
    "lint": "eslint .",
    "test": "jest",
    "test:watch": "npm test -- --watch",
    "storybook": "start-storybook -p 9001",
    "build-storybook": "NODE_ENV=storybook build-storybook -c .storybook -o storybook-build",
    "deploy-storybook": "storybook-to-ghpages"
  },
  "main": "lib",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/insane-ux/rebulma.git"
  },
  "license": "MIT",
  "bugs": {
    "url": "git+https://github.com/insane-ux/rebulma.git/issues"
  },
  "dependencies": {
    "classnames": "^2.2.5",
    "react-css-themr": "^2.0.0"
  },
  "devDependencies": {
    "@kadira/storybook": "^2.35.3",
    "babel-eslint": "^7.2.3",
    "babel-jest": "^19.0.0",
    "babel-plugin-module-resolver": "^2.7.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-stage-0": "^6.24.1",
    "enzyme": "^2.8.2",
    "eslint": "^3.19.0",
    "eslint-config-airbnb": "^14.1.0",
    "eslint-import-resolver-webpack": "^0.8.1",
    "eslint-plugin-flowtype": "^2.32.1",
    "eslint-plugin-import": "^2.2.0",
    "eslint-plugin-jsx-a11y": "^4.0.0",
    "eslint-plugin-react": "^6.9.0",
    "flow-bin": "^0.45.0",
    "jest-cli": "^19.0.2",
    "react": "^15.5.4",
    "react-addons-test-utils": "^15.5.1",
    "react-dom": "^15.5.4",
    "react-test-renderer": "^15.5.4",
    "recompose": "^0.23.1"
  }
}
